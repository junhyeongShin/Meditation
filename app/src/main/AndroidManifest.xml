<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.meditation">

    <uses-permission android:name="android.permission.CAMERA"/> <!-- 카메라 권한 -->
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.SET_ALARM"/>
    <uses-permission android:name="android.permission.WRITE_CALENDAR"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE"/>

    <uses-feature
            android:name="android.hardware.camera"
            android:required="true"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".InputEventActivity">
        </activity>
        <activity android:name=".CalendarActivity">
        </activity>
        <activity android:name=".MainActivity2">
        </activity>
        <activity android:name=".ContactInput">
        </activity>
        <activity android:name=".ContactEx">
        </activity>
        <activity android:name=".MyPage">
        </activity>
        <activity android:name=".InputActivity">
            <intent-filter>
                <action android:name="android.intent.action.INSERT"/>

                <data android:mimeType="vnd.android.cursor.dir/event"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".MeditationPlay">
        </activity>
        <activity android:name=".Timer">
            <intent-filter>
                <action android:name="android.intent.action.SET_TIMER"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".Alarm">
            <intent-filter>
                <action android:name="android.intent.action.SET_ALARM"/>

                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".View_Test">
        </activity>
        <activity android:name=".Camera">
        </activity>
        <activity android:name=".MedtationPlayer">
        </activity>
        <activity android:name=".contentsInput">
        </activity>
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".data.Login">
        </activity>
        <activity android:name=".data.Sign">
        </activity>

        <receiver android:name=".Alarm_Receiver"/>

        <service
                android:name=".RingtonePlayingService"
                android:enabled="true">
        </service>
    </application>

</manifest>